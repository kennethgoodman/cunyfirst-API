<div class="container">
  <!-- Modal -->
  <div class="modal fade" id="confirmDialog" role="dialog">
    <div class="modal-dialog">
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Confirmation On Classes</h4>
        </div>
        <div class="modal-body" id="groupsTable">
            <div class="tableHolder">
                <table id="groupTable" cellspacing="0">
                    <thead>
                        <tr>
                            <th>Dept</th>
                            <th>Class nbr</th><th>Class Section</th><th>Teacher</th>
                            <th>Days and Time</th><th>Group</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>Dept</th>
                            <th>Class nbr</th><th>Class Section</th><th>Teacher</th>
                            <th>Days and Time</th><th>Group</th>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
        <div class="modal-body" id="groupCount" style="text-align:center" ng-app="countApp">
            <form name="inputForm" novalidate>
                <input type="text" style="width: 65%;" class="groupCount" id="groupCountId" name="countModel" ng-model="countModel" placeholder="How many classes do you want to take, examples: 5, 7, 5-7" ng-pattern="/^([1-9]|1[0-4])\s*$|^([1-9]|1[0-5])\s?-{1}\s?([1-9]|1[0-4]){1}\s*$/">
            <div ng-messages="inputForm.countModel.$error">
                <div ng-message="pattern">Must be a number between 1 and 14 or a number followed by a dash and another number, example: "5-7"</div>
            </div><br>
            </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" class="btn btn-success" id="getScheduleConfirm">Confirm</button>           
        </div>
      </div>
    </div>
  </div>
</div>
<div class="container">
  <!-- Modal -->
  <div class="modal fade" id="userPreferencesDialog" role="dialog">
    <div class="modal-dialog">
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">User Preferences</h4>
        </div>
        <div class="modal-body" id="modelForm">
          <form>
              <div>
                <p>
                  <label for="DaysInSchool">Days In School Range:</label>
                   <span class="slider-time">10:00 AM</span> - <span class="slider-time2">4:00 PM</span>
                  <!--input type="text" id="DaysInSchool" readonly style="border:0; color:#f6931f; font-weight:bold;"--><br>
                  <div id="DaysInSchoolslider-range" style="width:50%"></div>
                </p>
              </div>
              <br>
            <p>
              Which weekend days can you go to school on?
            </p>
            <form class="form-inline">
            <label class="checkbox-inline"><input id="fridayMorning" type="checkbox" value="">Friday Morning/Afternoon (before 3pm)</label>
            <label class="checkbox-inline"><input id="fridayNight" type="checkbox" value="">Friday Night</label>
            <label class="checkbox-inline"><input id="saturday" type="checkbox" value="">Saturday</label>
            <label class="checkbox-inline"><input id="sunday" type="checkbox" value="">Sunday</label>
            </form>
            <br>
            <div id="time-range">
                <p>Times you are willing to spend in school: <span class="slider-time">10:00 AM</span> - <span class="slider-time2">4:00 PM</span></p><br>
                <div class="sliders_step1">
                    <div id="slider-range"></div>
                </div>
            </div>
          </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" id="backButton">Back</button>
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" class="btn btn-success" id="preferencesConfirm">Confirm</button>           
        </div>
      </div>
    </div>
  </div>
</div>