#! /app/.heroku/node/bin/node
function sayHello(){
	var Heroku = require('heroku-client'),heroku = new Heroku({ token: process.env.HEROKU_API_TOKEN });
	var hk = Heroku.createClient({token:process.env.HEROKU_API_TOKEN});
	console.log(hk.apps(process.env.HEROKU_APP_NAME).dynos().list());
	var app = heroku.apps(process.env.HEROKU_APP_NAME);
	app.info(function (err, app) {
		console.log('Hello');
		console.log(err);
		console.log(app);
	});
}
sayHello();
process.exit(0);