<div class="container">
  <!-- Modal -->
  <div class="modal fade" id="confirmDialog" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Confirmation On Classes</h4>
        </div>
        <div class="modal-body" id="alreadySignedUpClasses">
          
        </div>
        <div class="modal-body" id="confirmInfo">
            
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" id="backButton">Back</button>
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" class="btn btn-success" id="confirm">Confirm</button>           
        </div>
      </div>
    </div>
  </div>
  
</div>
<div class="container">
  <!-- Modal -->
  <div class="modal fade" id="infoDialog" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content" ng-app="userInfoApp" ng-controller="userInputControl">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Information</h4>
        </div>
        <div class="modal-body" id="confirmInfo">
            <form name="inputForm" novalidate>
                <!--<span ng-show="!carrierAndNumAreValid()">
                     Either enter a valid number or e-mail. <br>If you enter a phone number, <br>you must enter a carrier.
                </span> -->
                <p>Fill out your phone number and provider.</p><br>      
                <input type="text" placeholder="10 Digit phone number" id="you_phone_nbr" name="phoneNbrModel" ng-model="phoneNbrModel" ng-pattern="/^[\+]?[(]?[0-9]{3}[)]?[-\s\.]?[0-9]{3}[-\s\.]?[0-9]{4,6}$/">
                <div ng-messages="inputForm.phoneNbrModel.$error">
                    <div ng-message="pattern">Must be a valid 10 digit phone number</div>
                </div><br>
                <select id="carrier" default="pick a carrier" ng-model="carrierModel" name="carrierModel">
                    <option value="defualt" selected="selected">
                        Pick A Carrier
                    </option> 
                </select>
                <!-- img src="/images/ajax-loader.gif" id="ajax-loader" style="display:none" -->
            </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" class="btn btn-default" id="next" ng-disabled="!carrierAndNumAreValid()">Next!</button>
        </div>
      </div>
    </div>
  </div>
  
</div>
