<!DOCTYPE html>
<html>
<head>
  
</head>

<body>


<div class="container">
<h2>Database Results:</h2>

    
    
<table>
   <tr>
      <th>Name</th><th>Inst</th><th>Dept</th><th>Class</th><th>Section</th><th>Texted</th><th>phone number</th><th>Session</th>
   </tr>

   <% results.forEach(function(r) { %>
    <tr>
        <% for(var data in r){ %>
            <td><%=r[data]%></td>
        <% } %>
    </tr>    
   <% }); %>
</table>    

</div>

</body>
</html>
